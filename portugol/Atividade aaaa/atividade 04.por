programa {
  funcao inicio() {
     caracter letra

     escreva("escreva a primeira letra que vier em sua mente: ")
     leia(letra)

     se ((letra =='a') ou (letra =='e') ou (letra =='i') ou (letra =='o') ou (letra =='u')){
     escreva("sua letra é uma vogal")
     }

     senao {
      escreva("Não e uma vogal")
     }
  }
}
