programa {
  funcao inicio() {
    inteiro voto

    escreva("13 - lula \n")
    escreva("22 - Bolsonaro \n")
    escreva("56 - eneas \n")
    leia (voto)

    escolha(voto){
      caso 13:
       escreva("Faz o éli: que vergonha companheiro ")

      pare

      caso 22:
       escreva("Faz a arminha:")

      pare

      caso 56:
       escreva("Faz a bomba atomika")

      pare

      caso contrario:
       escreva("Escolha um número valido") 

    }
  }
}
